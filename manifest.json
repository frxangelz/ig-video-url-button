{
  "manifest_version": 2,

  "name": "Instagram Download Button - FreeAngel",
  "description": "Add Download Button video",
  "version": "1.0",
  "homepage_url": "http://www.youtube.com/channel/UC15iFd0nlfG_tEBrt6Qz1NQ",	
  "browser_action": {
    "default_icon": "icon19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
	"<all_urls>"
  ],
  
  "content_scripts": [
    {
      "matches": [
		"<all_urls>"
      ],
      "js": [ "inject.js" ]
    }
  ]  
}